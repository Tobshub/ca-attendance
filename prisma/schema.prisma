//   password String
//   name     String
// }

model Member {
  id         Int       @id @default(autoincrement())
  name       String
  phoneNum   String
  address    String?
  sex        Sex
  present    Service[]
  dateJoined DateTime?
}

model Service {
  id      Int      @id @default(autoincrement())
  date    DateTime @unique
  name    String
  present Member[]
}

enum Sex {
  MALE
  FEMALE
}
